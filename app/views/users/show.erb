<%= link_to "Log out", logout_path, method: "delete" %>
<h1> <%= @user.username %>'s Dashboard </h1>



<h2> Saved Trips </h2>
<h3> Click on a trip for more information and photos </h3>
<% @user.trips.each do |trip| %>
    <li><%= link_to trip.location, trip_path(@user, trip) %></li>
 <% end %> <br>

 


<%= link_to "Add a trip", new_trip_path(@user) %> <br>