<h1>Add a new trip </h1>

<%= form_for @trip do |f| %>
    <%= f.label :location %>
    <%= f.text_field :location %> <br>

    <%= f.label :date %>
    <%= f.text_field :date %> <br>
 
    <%= f.submit "Submit" %> <br>
    <%= f.hidden_field :user_id, :value => session[:user_id] %>

<% end %>