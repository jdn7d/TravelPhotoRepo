<h1> <%= @user.username %> Add a new trip </h1>

<%= form_for ([@trip]) do |f| %>
    <%= f.label :location %>
    <%= f.text_field :location %> <br>

    <%= f.label :date %>
    <%= f.text_field :date %> <br>
 
    <%= f.label :cover_photo %>
    <%= f.file_field :cover_photo %> <br>

    <%= f.hidden_field :user_id, :value => @user.id %>

    <%= f.submit "Submit" %>

<% end %>