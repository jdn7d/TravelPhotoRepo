<h1> Add a new photo </h1>

<%= form_for @photo do |f| %>
    <%= f.label :category %>
    <%= f.text_field :category %> <br>

    <%= f.label :file %>
    <%= f.file_field :file %> <br>
    
    <%= f.hidden_field :trip_id,  :value => @trip.id %>

    <%= f.submit "Submit" %>

<% end %>

